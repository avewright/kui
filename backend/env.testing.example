# Testing Environment Configuration Example
# Copy this to .env or set these as environment variables for testing

ENVIRONMENT=testing
DEBUG=false
ALLOW_DUMMY_DATA=true
FORCE_DUMMY_DATA=true

# Server Configuration
HOST=127.0.0.1
PORT=8080

# AI Service Configuration (not used when FORCE_DUMMY_DATA=true)
AI_SERVICE_URL=http://127.0.0.1:5000
AI_SERVICE_TIMEOUT=30 